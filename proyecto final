		   cout<<"TOTAL ["<<TOTAL<<" Bs.]"<<endl;
		   cout<<"--------------------------------------------------------------"<<endl;
		  

		   Registros.setArchivo(nombreCliente, correo, telefono,
	       factura, ubicacion, recomendacion, NIT);

			   break;
	   default : cout<<"             OPCION INVALIDA"<<endl;break;
}

