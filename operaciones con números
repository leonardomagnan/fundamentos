#include <iostream>

using namespace std;

int main()
{
    int a,b,c ;
    cout<< "ingrese el primer dato: ";
    cin>>a;
    cout<< "ingrese el segundo dato: ";
    cin>>b ;
    c=a+b ;
    cout<< "el resultado es: "<<c <<endl;
    c=a-b ;
    cout<< "la resta es: "<<c <<endl;
    c=a*b;
    cout<< "la multiplicacion es: "<<c <<endl;
    c=a/b;
    cout<< "la division es: "<<c <<endl;
    c=a%b;
    cout<<"el modulo es: "<<c <<endl;
    

    return 0 ;
}
